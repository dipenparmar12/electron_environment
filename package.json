{
  "name": "itemlist",
  "version": "1.0.0",
  "repository": {
    "type": "git"
  },
  "description": "Electron App for item list ",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "postinstall": "electron-builder install-app-deps",
    "build": "electron-builder -wml",
    "build-win": "electron-builder -w",
    "build-mac": "electron-builder -m",
    "build-linux": "electron-builder -l",
    "dist": "build"
  },
  "build": {
    "directories": {
      "buildResources": "assets/icons"
    },
    "linux": {
      "category": "Utility"
    }
  },
  "author": {
    "name": "Dipen Parmar",
    "email": "dipensavji@gmail.com",
    "url": "https://github.com/dipenparmar12"
  },
  "license": "ISC",
  "devDependencies": {
    "electron": "^4.1.3",
    "electron-builder": "^20.39.0",
    "electron-download-tf": "^4.3.4"
  },
  "dependencies": {
    "electron-settings": "^3.2.0"
  }
}
